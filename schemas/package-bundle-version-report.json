    "PackagingSObjects.SubscriberPackageDependencies": {
      "type": "object",
      "properties": {
        "ids": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "subscriberPackageVersionId": {
                "type": "string"
              }
            },
            "required": ["subscriberPackageVersionId"],
            "additionalProperties": false
          }
        }
      },
      "required": ["ids"],
      "additionalProperties": false
