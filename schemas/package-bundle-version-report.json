          "type": "boolean"
        },
        "CreatedDate": {
          "type": "string"
        },
        "CreatedById": {
          "type": "string"
        },
        "LastModifiedDate": {
          "type": "string"
        },
        "LastModifiedById": {
          "type": "string"
        }
      },
      "required": [
        "Id",
        "PackageBundle",
        "VersionName",
        "MajorVersion",
        "MinorVersion",
        "IsReleased",
        "CreatedDate",
        "CreatedById",
        "LastModifiedDate",
        "LastModifiedById"
      ],
      "additionalProperties": false
    }
